#!/bin/sh
# This file is automatically generated by fs-package

# FIXME: Depends on libuuid


set -e

fsdeps/packages/freetype
fsdeps/packages/libexpat

PACKAGE=fontconfig-2.13.1
URL=https://www.freedesktop.org/software/fontconfig/release/$PACKAGE.tar.gz
SHA256=9f0d852b39d75fc655f9f53850eb32555394f36104a044bb2b2fc9e66dbbfa7f
REVISION=0

. fsdeps/dep.sh

python3 fsdeps/download.py "$URL" sha256:$SHA256

rm -Rf fsdeps/_build && mkdir fsdeps/_build && cd fsdeps/_build
tar xf ../_sources/$PACKAGE.tar.gz
cd $PACKAGE

./configure --prefix=$PREFIX
make VERBOSE=1
make install

touch $INSTALLED
