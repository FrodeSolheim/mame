#!/bin/sh
# This file is automatically generated by fs-package

set -e

PACKAGE=expat-2.4.8
URL=https://github.com/libexpat/libexpat/releases/download/R_2_4_8/$PACKAGE.tar.xz
SHA256=f79b8f904b749e3e0d20afeadecf8249c55b2e32d4ebb089ae378df479dcaf25
REVISION=0

. fsdeps/dep.sh

python3 fsdeps/download.py "$URL" sha256:$SHA256

rm -Rf fsdeps/_build && mkdir fsdeps/_build && cd fsdeps/_build
tar xf ../_sources/$PACKAGE.tar.xz
cd $PACKAGE

./configure --prefix=$PREFIX
make
make install

touch $INSTALLED
