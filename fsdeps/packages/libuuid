#!/bin/sh
# This file is automatically generated by fs-package

set -e

PACKAGE=libuuid-2.38
URL=https://mirrors.edge.kernel.org/pub/linux/utils/util-linux/v2.38/util-linux-2.38.tar.xz
SHA256=6d111cbe4d55b336db2f1fbeffbc65b89908704c01136371d32aa9bec373eb64
REVISION=0

. fsdeps/dep.sh

if [ $SYSTEM_OS = "Linux" ]; then

python3 fsdeps/download.py "$URL" sha256:$SHA256

rm -Rf fsdeps/_build && mkdir fsdeps/_build && cd fsdeps/_build
tar xf ../_sources/util-linux-2.38.tar.xz
cd util-linux-2.38

./configure --prefix $PREFIX \
--disable-all-programs --enable-libuuid
make
make install

fi

touch $INSTALLED
